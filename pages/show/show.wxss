.container {
  margin: 0rpx 16rpx;
}

.block {
  background-color: #F5F5F5;
  border-radius: 40rpx;
  font-size: 32rpx;
  /* position: relative; */
}

.title {
  font-size: 18px;
  position: relative;
}

.title-name {
  padding-top: 48rpx;
  padding-left: 48rpx;
}

.title-color { 
  background-color: #256657;
  opacity: 70%;
  height: 4px;
  width: 73px;
  position: absolute;
  bottom: 4px;
  margin-left: 48rpx;
}

.summary {
  margin: 32rpx 48rpx 64rpx 48rpx;
  padding-bottom: 64rpx;
}

#info {
  margin-bottom: 80rpx;
  border: 1px solid #000000;
}

.website {
  padding-top: 36rpx;
  padding-bottom: 60rpx;
  margin-left: 48rpx;
  display: flex;
  justify-content: end;
  margin-right: 20rpx;
}

.icon {
  height: 32rpx;
  width: 36rpx;
  margin-top: 6rpx;
  margin-right: 10rpx;
}
.video{
  min-width: 100%;
}

.links {
  display: flex;
}

.banner {
  padding-top: 36rpx;
  margin-left: 48rpx;
}

.button-next-brand {
  width: 686rpx;
  height: 148rpx;
  background-color:black;
  border-radius: 40rpx;
  color:white;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  margin-bottom: 80rpx;
}
.button-home{
  width: 686rpx;
  height: 146rpx;
  color:black;
  border-radius: 40rpx;
  border: 1px solid #000000;
  background-color:white;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  margin-top: 160rpx;
  margin-bottom: 40rpx;
}
.linebreak {
  white-space: pre-line;
}