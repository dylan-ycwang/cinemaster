.semicircle {
  width: 200vw;
  transform: translateY(-140rpx);
  position: fixed;
  left: -25vw;
  z-index: -2;
}
.logo-container{
  height: 140rpx;
  transform: translateY(-120rpx);
}

.logo{
  height: 120rpx;


}
